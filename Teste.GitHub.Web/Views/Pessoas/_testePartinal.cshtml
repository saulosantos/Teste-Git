


@Scripts.Render("~/bundles/jquery")




<script>
    $(function () {
        $.ajax({
            dataType: "json",
            type: "GET",
            url: "/Pessoas/listarImagens/40",
            success: function (dados) {
                $(dados).each(function (i) {
                    document.writeln("<p>Nome: " + dados[i].PessoaId + " | URL: " + dados[i].Nome + " | URL: " + dados[i].Endereco + " | URL: " + dados[i].CPF + " | URL: " + dados[i].Email + " | URL: " + dados[i].DataNascimento + " | URL: " + dados[i].Telefone + " | URL: " + dados[i].DataCadastro + " | URL: " + dados[i].Ativo + " | URL: " + dados[i].ArquivoPessoaId + ""  )
                });
            }
        });
    });
</script>
